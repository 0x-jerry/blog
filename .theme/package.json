{
  "name": "@0x-jerry/vitepress-theme-blog",
  "description": "My Blog Theme",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/blog.git"
  },
  "bugs": {
    "url": "https://github.com/0x-jerry/blog/issues"
  },
  "type": "module",
  "homepage": "https://github.com/0x-jerry/blog#readme",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./config": {
      "types": "./config.d.ts",
      "default": "./dist/config.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsup",
    "release": "x-release",
    "dev": "tsup --watch vitepress"
  },
  "tsup": {
    "entry": [
      "vitepress/config.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true
  },
  "dependencies": {
    "@0x-jerry/unocss-preset-brand": "^0.1.6",
    "@0x-jerry/utils": "^5.0.0",
    "@0x-jerry/vue-kit": "^2.3.1",
    "@iconify-json/carbon": "^1.2.14",
    "@iconify-json/logos": "^1.2.10",
    "@mdit/plugin-figure": "^0.22.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@vueuse/core": "^14.0.0",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.19",
    "feed": "^5.1.0",
    "gray-matter": "^4.0.3",
    "heti": "^0.9.6",
    "i18next": "^25.6.3",
    "i18next-vue": "^5.3.0",
    "less": "^4.4.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pagefind": "^1.4.0",
    "reading-time": "^1.5.0",
    "sass": "^1.94.1",
    "unocss": "66.5.7",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.2",
    "vitepress": "^1.6.4",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^2.4.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.1",
    "@types/nprogress": "^0.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}